|  \\    /   O peration     | Version:     4.0                                |
|   \\  /    A nd           | Web:         http://www.foam-extend.org         |
|    \\/     M anipulation  |                                                 |
\*---------------------------------------------------------------------------*/
FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      reactions;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

species
(
	electron
	Arp1
    Ar2p1
	Arm
	Ar
);

reactions
(
    // ionization 1
    electronImpactInelasticTabularReaction
    electron + Ar = electron + electron + Arp1
    ("$FOAM_CASE/constant/reaction_ion_1")
    (2.52487E-18)

    // metastable 1
    electronImpactInelasticTabularReaction
    electron + Ar = electron + Arm
    ("$FOAM_CASE/constant/reaction_exc")
    (1.850514E-18)

    // ionization 2
    electronImpactInelasticTabularReaction
    electron + Arm = electron + electron + Arp1
    ("$FOAM_CASE/constant/reaction_ion_2")
    (6.63301e-19)

    // arrhenius 1
    irreversibleArrheniusReaction
    Arm + Arm = electron + Arp1 + Ar
    (1.25165E13 -0.5 0)

    // arrhenius 2
    irreversiblethirdBodyArrheniusReaction
    Arp1 + Ar + Ar = Ar2p1 + Ar
    ((4.71089E14 -1.5 0) (1))

    // dissociative recombination
    electronImpactInelasticArrheniusReaction
    electron + Ar2p1 = Arm + Ar
    (7.30129E15 -0.5 0)
    (4.14E-21)

    // photoionization
    irreversibleArrheniusReaction
    Arm = Ar
    (5E5 0 0)

    // elastic collision
    electronImpactElasticTabularReaction
    electron + Ar = electron + Ar
    ("$FOAM_CASE/constant/elastic")
);
