FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      Phi;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [ 0 0 0 1 0 0 0 ];

internalField   uniform 0;

boundaryField
{
    electrode
    {
        type externalCircuit1D;
	    amplitude 800;
	    model cosFrequencyModulated;
	    frequency 13.56e6;
	    bias 0;
	    R 50;
    }
    ground
    {
        type            fixedValue;
        value           uniform 0;
    }
    otherfaces
    {
        type            empty;
    }
}


