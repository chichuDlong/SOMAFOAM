FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      Phi;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [ 0 0 0 1 0 0 0 ];

internalField   uniform 0.1;

boundaryField
{
    ground
    {
        type            fixedValue;
        value           uniform 0;
    }
    dielectric_1
    {
        type            coupledPotential;
	surfaceCharge   on;
	remoteField	Phi;
        value           uniform 0.1;
    }
    otherfaces
    {
        type            empty;
    }
}
