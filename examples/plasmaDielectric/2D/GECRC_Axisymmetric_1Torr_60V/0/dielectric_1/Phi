FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      Phi;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [ 0 0 0 1 0 0 0 ];

internalField   uniform 0.1;

boundaryField
{
    //outer
    //{
    //    type            zeroGradient;
    //    value           uniform 0.22;
    //}
    //electrode
    //{
    //    type            fixedValue;
    //    value           uniform 0.215;
    //}
    outer
    {
        type    zeroGradient;
    }
    ground
    {
        type    fixedValue;
        value   uniform 0;
    }
	electrode
    {
        type            plasmaPotential;
        value           uniform 0.1;
        model           sinFrequencyModulated;
        amplitude       uniform 60.0;
        frequency       13.56E6;
        bias            0.0;
    }

    plasma_1
    {
		type            coupledPotential;
		surfaceCharge	off;
     		value           uniform 0.1;
        	remoteField     Phi;
    }
    frontandback_d11
    {
        type            wedge;
    }
    frontandback_d12
    {
        type            wedge;
    }
}


