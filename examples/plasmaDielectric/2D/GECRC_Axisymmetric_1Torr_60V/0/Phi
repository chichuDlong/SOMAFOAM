FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      Phi;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [ 0 0 0 1 0 0 0 ];

internalField   uniform 0.1;

boundaryField
{
    ground
    {
        type            fixedValue;
        value           uniform 0;
    }
    electrode
    {
        type            plasmaPotential;
        value           uniform 0.1;
        model           sinFrequencyModulated;
        amplitude       uniform 60.0;
        frequency       13.56E6;
        bias            0.0;
    }
    dielectric_1
    {
        type            coupledPotential;
	    surfaceCharge   on;
	    remoteField	    Phi;
        value           uniform 0.1;
    }
    dielectric_2
    {
        type            coupledPotential;
        surfaceCharge   on;
        remoteField     Phi;
        value           uniform 0.1;
    }
    far
    {
        type            fixedValue;
        value           uniform 0;
    }
    frontandback1
    {
        type            wedge;
    }
    frontandback2
    {
        type            wedge;
    }
}
